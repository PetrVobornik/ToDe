<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ToDe.NastaveniPage">
    <ContentPage.Content>
        <Grid>
            <TableView Intent="Settings" Margin="20" x:Name="tvNastaveni">
                <TableRoot>
                    <TableSection Title="Audio">
                        <SwitchCell x:Name="sZvuky" Text="Zvukové efekty" On="{Binding PrehravatZvuky}"  />
                    </TableSection>
                </TableRoot>
            </TableView>
        </Grid>
    </ContentPage.Content>
</ContentPage>